{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "indent":["error", 2],
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
